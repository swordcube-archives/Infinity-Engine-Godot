;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Infinity Engine"
run/main_scene="res://scenes/TitleScreen.tscn"
run/delta_smoothing=false
boot_splash/image="res://Assets/Images/Splash Screen.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://Assets/Images/iconOG.png"
config/windows_native_icon="res://Assets/Images/icon.ico"

[audio]

default_bus_layout=""

[autoload]

Conductor="*res://scripts/Conductor.gd"
AudioHandler="*res://scenes/audio/AudioHandler.tscn"
Preferences="*res://scripts/Preferences.gd"
VolumeControl="*res://scenes/ui/VolumeControl.tscn"
InfoDisplay="*res://scenes/ui/InfoDisplay.tscn"

[display]

window/size/width=1280
window/size/height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray(  )

[global]

mouse=false

